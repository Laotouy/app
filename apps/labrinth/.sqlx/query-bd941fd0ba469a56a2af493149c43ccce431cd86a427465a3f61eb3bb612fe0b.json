  "query": "\n        SELECT pr.id, pr.user_id, pr.review_type, pr.new_value,\n               u.username, u.avatar_url, u.raw_avatar_url\n        FROM user_profile_reviews pr\n        JOIN users u ON u.id = pr.user_id\n        WHERE pr.status = 'pending'\n        ORDER BY pr.created_at ASC\n        FOR UPDATE OF pr\n        ",
